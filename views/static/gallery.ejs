<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="img/favicon1.png" type="image/png" />

    <link rel="stylesheet" href="/css/main.css" />
    <title><%= title %> </title>
  </head>
  <body>
    <%- include('../components/nav.ejs', {page: 'gallery'}) %>


    <div class="container">
      <div class="galereya py-5">
        <div class="row">
          <% rows.forEach(row => { %>
            <div class="col-lg-4 col-md-6 col-sm-12">
               <% row.forEach(img => { %>
                <a href="<%= img %> " data-fancybox="images" class="gallery__item img-wrapper rounded" data-caption="Caption for single image">
                  <img src="<%= img %> " alt="Caption for single image" />
                </a>  
               <% }) %>
            </div>
          <% }) %>
         
        </div>
      </div>
    </div>

    <%- include('../components/footer.ejs') %>

    <script src="/scripts/main.js"></script>
  </body>
</html>
